            <entry key="config" type="String">#cloud-config
bootcmd:
 - ln -s -t /etc/rc.d /etc/rc.local
hostname: GNS3-lab01
manage_etc_hosts: true
runcmd:
 - systemctl start rc-local
 - sed -i "s/PasswordAuthentication no/PasswordAuthentication yes/" /etc/ssh/sshd_config
 - echo "UseDNS no" &gt;&gt; /etc/ssh/sshd_config
 - service ssh restart
 - service sshd restart
groups:  
 - admin
users:  
 - default  
 - name: rlaney  
   ssh-authorized-keys:
     - ssh-rsa  rlaney@MonsterMacBook.local
   lock_passwd: false  
   groups: admin  
   passwd: ralrox22
   sudo: ["ALL=(ALL) NOPASSWD:ALL"]   
write_files:
- path: /etc/rc.local
  owner: root:root
  permissions: '0755'
  content: |-
    #!/bin/sh
    ifconfig lo0 up 192.168.0.10 netmask 255.255.255.255
    ifconfig eth1 up 10.10.0.10 netmask 255.255.0.0
    ifconfig eth2 up 10.20.0.10 netmask 255.255.0.0
    route add -host 192.168.0.2 gw 10.10.0.26 dev eth1
    route add -host 192.168.0.3 gw 10.10.0.26 dev eth1
    route add -host 192.168.0.4 gw 10.10.0.26 dev eth1
    route add -host 192.168.0.5 gw 10.10.0.26 dev eth1
    route add -net 10.10.0.0/16 gw 10.10.0.26 dev eth1
    route add -net 192.168.0.0/24 gw 10.10.0.26 dev eth1
    route add default gw 10.20.0.1 dev eth2
    exit 0</entry>
        </extensions>
